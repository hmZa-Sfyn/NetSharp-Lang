using class Logger from "Logger.ns";
class Point {
  public int X;
  public static Point operator+(Point a, Point b) {
    return new Point { X = MathUtils.Add(a.X, b.X) };
  }
}
class Program {
  shared void Main() {
    var logger = new Logger("Point");
    var p1 = new Point { X = 3 };
    var p2 = new Point { X = 4 };
    var sum = p1 + p2;
    logger.Log($"Sum X: {sum.X}");
  }
}